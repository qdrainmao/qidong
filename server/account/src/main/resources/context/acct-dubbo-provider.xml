<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
             http://www.springframework.org/schema/beans/spring-beans.xsd
                http://code.alibabatech.com/schema/dubbo
                     http://code.alibabatech.com/schema/dubbo/dubbo.xsd" >

    <dubbo:application name="acct_provider"/>

    <dubbo:registry address="zookeeper://192.168.0.103:2181" file="/dubbo-registry/dubbo-registry.properties"/>

    <dubbo:protocol name="dubbo" port="29001"/>

    <dubbo:service interface="com.account.service.dubbo.UserDubboService" ref="userDubboService" retries="0" timeout="1000"/>
</beans>